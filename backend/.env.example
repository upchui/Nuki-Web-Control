NUKI_API_TOKEN="your_nuki_web_api_token_here"

# Nuki API URL - Use official API or MQTT emulator
# Official API: https://api.nuki.io
# MQTT Emulator: http://nuki-mqtt-emulator:8001
NUKI_API_URL="https://api.nuki.io"

# Initial Admin User (will be created as admin in database)
INITIAL_ADMIN_USER="admin:admin123"

# Login Users (username:password) - will be migrated to database as regular users
#LOGIN_USERS="admin:admin123,user:test"

# JWT Secret for token signing
JWT_SECRET="nuki-web-secret-key-change-this-in-production-2025"

# Log Collection Settings
LOG_FETCH_INTERVAL=60
MAX_LOGS_PER_SMARTLOCK=500
ENABLE_LOG_COLLECTION=true
