# MQTT Broker Configuration
MQTT_HOST=<mqtt-server>
MQTT_PORT=1883
MQTT_USERNAME=<mqtt-user>
MQTT_PASSWORD=<mqtt-password>
MQTT_TOPIC_PREFIX=nuki

# Optional: Client ID for MQTT connection
MQTT_CLIENT_ID=nuki-emulator

# Logging Level
LOG_LEVEL=INFO

# Log Collection Configuration
LOG_COLLECTION_ENABLED=true
LOG_COLLECTION_INTERVAL=60
MAX_LOGS_PER_SMARTLOCK=500
LOG_STORAGE_PATH=/data-emulator/logs
